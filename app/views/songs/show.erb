<div class='container' style="padding-top: 20px;">
  <div class='row'>
    <div class='span8'>
      <div class="page-header">
        <h1>
          <%= @song.title %>
        </h1>
      </div>
      <table class='table table-striped'>
        <tr>
          <td>Artist</td>
          <td><%= @song.artist %></td>
        </tr>
        <tr>
          <td>Album</td>
          <td><%= @song.album %></td>
        </tr>
        <tr>
          <td>Genre</td>
          <td><%= @song.genre %></td>
        </tr>
        <tr>
          <td>Length</td>
          <td><%= @song.length %></td>
        </tr>
      </table>
      <a href="<%= @song.id %>/edit">Edit</a>
    </div>
  </div>
</div>
